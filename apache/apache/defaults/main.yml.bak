apache_default_ServerRoot: "/etc/httpd"
apache_default_Listen: "80"
apache_default_User: "apache"
apache_default_Group: "apache"
apache_default_ServerAdmin: "root@localhost"
apache_default_ServerName: "www.example.com:80"
apache_default_DocumentRoot: "/var/www/html"
apache_default_BaseDir: "/var/www"
apache_default_ErrorLog: "logs/error_log"
apache_default_LogLevel: "warn"
apache_default_LogFormat_combined: '%h %l %u %t \"%r\" %>s %b \"%{Referer}i\" \"%{User-Agent}i\"'
apache_default_LogFormat_common: '%h %l %u %t \"%r\" %>s %b'
apache_default_LogFormat_combinedio: '%h %l %u %t \"%r\" %>s %b \"%{Referer}i\" \"%{User-Agent}i\" %I %O'
apache_default_CustomLog: "logs/access_log"
apache_default_CGI_Ailias: "/cgi-bin/"
apache_default_CGI_Path: "/var/www/cgi-bin/"
apache_default_AddDefaultCharset: "UTF-8"
apache_default_home: "/usr/share/httpd"
apache_default_GroupID: "48"
apache_default_UserID: "48"
